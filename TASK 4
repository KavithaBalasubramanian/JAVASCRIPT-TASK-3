//Write a program to create a calculator with all arithmetic operations has functions;

function ADD(a,b){
sum=a+b
console.log(sum)
}
ADD(15,20);

function SUB(a,b){
sub=a-b
console.log(sub)
}
SUB(20,50);

function MUL(a,b){
mul=a * b
console.log(mul)
}
MUL(50,50);

function DIV(a,b){
div=a/b
console.log(div)
}
DIV(10,5)

function REM(a,b){
rem=a%b
console.log(rem)
}
REM(10,5)
